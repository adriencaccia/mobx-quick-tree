{
  "name": "@gadgetinc/mobx-quick-tree",
  "version": "0.1.0",
  "description": "Library that mirrors mobx-state-tree's API to construct a fast, read-only version with all the same views",
  "source": "src/index.ts",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "github:gadget-inc/mobx-quick-tree",
  "license": "MIT",
  "files": [
    "README.md",
    "dist/src/**/*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "node node_modules/.bin/jest --runInBand",
    "build": "rm -rf dist && tsc",
    "watch": "rm -rf dist && tsc --watch --preserveWatchOutput",
    "prepublishOnly": "yarn run build",
    "prerelease": "gitpkg publish"
  },
  "dependencies": {
    "mobx": "^6.5.0",
    "mobx-state-tree": "^5.1.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "jest": "^27.5.1",
    "typescript": "^4.6.2"
  }
}
